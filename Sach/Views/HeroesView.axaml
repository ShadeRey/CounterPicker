    <UserControl.Styles>
        <Style Selector="views|HeroButtonView.searched">
            <Setter Property="BorderThickness" Value="5"/>
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <RadialGradientBrush Radius="1.1">
                        <GradientStop Color="#53a09e" Offset="0" /> <!--  -->
                         <GradientStop Color="#365862" Offset="1" /> <!-- -->
                    </RadialGradientBrush>
                </Setter.Value>
            </Setter>
        </Style>
        <Style Selector="views|HeroButtonView.picked">
            <Setter Property="IsEnabled" Value="False" />
            <Setter Property="Opacity" Value="0.7" />
        </Style>
        <Style Selector="views|HeroButtonView.suggestion">
            <Setter Property="BorderThickness" Value="3" />
            <Setter Property="CornerRadius" Value="4" />
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <LinearGradientBrush>
                        <LinearGradientBrush.GradientStops>
                            <GradientStop Color="Aqua" Offset="0" />
                            <GradientStop Color="Brown" Offset="0.5" />
                        </LinearGradientBrush.GradientStops>
                        <LinearGradientBrush.Transform>
                            <RotateTransform Angle="0" />
                        </LinearGradientBrush.Transform>
                    </LinearGradientBrush>
                </Setter.Value>
            </Setter>
            <Style.Animations>
                <Animation
                    PlaybackDirection="Reverse"
                    FillMode="None"
                    IterationCount="Infinite"
                    Duration="0:0:2">
                    <Animation.Easing>
                        <LinearEasing />
                    </Animation.Easing>
                    <KeyFrame Cue="0%">
                        <Setter Property="BorderBrush">
                            <Setter.Value>
                                <LinearGradientBrush StartPoint="0%,100%" EndPoint="100%,100%">
                                    <GradientStop Offset="0" Color="Gold" />
                                    <GradientStop Offset="0" Color="Chocolate" />
                                    <GradientStop Offset="1" Color="Gold" />
                                </LinearGradientBrush>
                            </Setter.Value>
                        </Setter>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="BorderBrush">
                            <Setter.Value>
                                <LinearGradientBrush StartPoint="0%,100%" EndPoint="100%,100%">
                                    <GradientStop Offset="0" Color="Chocolate" />
                                    <GradientStop Offset="1" Color="Gold" />
                                    <GradientStop Offset="1" Color="Chocolate" />
                                </LinearGradientBrush>
                            </Setter.Value>
                        </Setter>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>
        <Style Selector="views|HeroButtonView">
            <Setter Property="HeroButtonCommand" Value="{Binding OnHeroButtonClickCommand}" />
        </Style>
        <Style Selector="views|HeroButtonView:pointerover">
            <Setter Property="Width" Value="100" />
            <Setter Property="Height" Value="140" />
            <Setter Property="ZIndex" Value="1" />
            <Style Selector="^ TextBlock">
                <Setter Property="IsVisible" Value="True" />
            </Style>
        </Style>
    </UserControl.Styles>

